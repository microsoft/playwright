+            modifiers,
+            WallTime::now(),
+            force,
+            syntheticClickType
+        ));
+        localMainFrame->eventHandler().handleMouseReleaseEvent(PlatformMouseEvent(
+            adjustedIntPoint,
+            adjustedIntPoint,
+            MouseButton::Left,
+            PlatformEvent::Type::MouseReleased,
+            1,
+            modifiers,
+            WallTime::now(),
+            force,
+            syntheticClickType
+        ));
+    }
