+ScreencastEncoder::ScreencastEncoder(std::unique_ptr<VPXCodec>&& vpxCodec, IntSize size)
+    : m_vpxCodec(WTFMove(vpxCodec))
+    , m_size(size)
+{
+    ASSERT(!size.isZero());
+}
