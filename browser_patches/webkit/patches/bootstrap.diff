+    if ([address rangeOfString:@"://"].length > 0)
+        return address;
+
+    if ([address hasPrefix:@"data:"])
+        return address;
+
+    if ([address hasPrefix:@"about:"])
+        return address;
+
+    return [@"http://" stringByAppendingString:address];
