+
+    bool allGoodCharacters = true;
+    for (unsigned i = 0; allGoodCharacters && i < cp.size(); ++i) {
+        const UChar c = cp[i];
+        allGoodCharacters = (c >= '0' && c <= '9') || (c >= 'A' && c <= 'Z') || (c >= 'a' && c <= 'z') || c == '_';
