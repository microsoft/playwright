+Inspector::Protocol::ErrorStringOr<String> InspectorPlaywrightAgent::getInfo()
+{
+#if PLATFORM(MAC)
+    return { "macOS"_s };
+#elif PLATFORM(GTK) || PLATFORM(WPE)
+    return { "Linux"_s };
+#elif PLATFORM(WIN)
+    return { "Windows"_s };
+#else
+#error "Unsupported platform."
+#endif
+}
