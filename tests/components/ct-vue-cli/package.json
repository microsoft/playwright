{
  "name": "ct-vue-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "typecheck": "tsc --noEmit --project tsconfig.test.json"
