{
    "manifest_version": 3,
    "name": "Console Log Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }