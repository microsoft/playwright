    "@types/rimraf": "^3.0.0",
    "@types/source-map-support": "^0.4.2",
    "@types/webpack": "^4.41.25",
    "@types/ws": "7.2.6",
    "@types/xml2js": "^0.4.5",
    "@types/yazl": "^2.4.2",
