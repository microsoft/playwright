    "file-loader": "^6.1.0",
    "formidable": "^1.2.2",
    "html-webpack-plugin": "^4.4.1",
    "ncp": "^2.0.0",
    "node-stream-zip": "^1.11.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "socksv5": "0.0.6",
    "style-loader": "^1.2.1",
    "typescript": "=4.2.4",
